{
    "metadata-file": "metadata.json",
    "output-feature": "target",
    "ignored-features": [],
    "learner-settings": {
        "mode": "classification",
        "reduction-strategy": "none",
        "grower-settings": {
            "forest-settings": {
                "type": "ClassicForest",
                "number-of-trees": 8,
                "sampling-proportion": 1,
                "oob-proportion": 0.05
            },
            "tree-settings": {
                "type": "GreedyNarrow1D",
                "feature-proportion": "sqrt",
                "max-depth": 12,
                "desired-leaf-size": 64
            }
        }
    },
    "training-dataset": {
        "type": "CSV",
        "path": "../uploads/train.csv"
    },
    "validation-settings": {
        "type": "CV",
        "number-of-runs": 1,
        "number-of-partitions": 10
    }
}